{
    "alias": ["fedor.me"],
    "builds": [
        {
            "src": "*.html",
            "use": "@now/html-minifier"
        },
        {
            "src": "assets/css/*.css",
            "use": "@now/static"
        },
        {
            "src": "resume.pdf",
            "use": "@now/static"
        },
        {
            "src": "robots.txt",
            "use": "@now/static"
        },
        {
            "src": "sitemap.xml",
            "use": "@now/static"
        }
    ],
    "github": {
        "autoAlias": true,
        "autoJobCancelation": true,
        "enabled": true,
        "silent": true
    },
    "name": "fedor-website",
    "regions": ["all"],
    "version": 2
}
